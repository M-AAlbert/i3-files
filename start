Keyboard settings

# Read and parsed by systemd-localed. It's probably wise not to edit this file
# manually too freely.
Section "InputClass"
        Identifier "system-keyboard"
        MatchIsKeyboard "on"
        Option "XkbLayout" "ca"
        Option "XkbModel" "pc105"
        Option "XkbVariant" "multix"
EndSection

